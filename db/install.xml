<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/inventario/db" VERSION="2025122200" COMMENT="XMLDB file for local_inventario" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
    <TABLES>
        <TABLE NAME="local_inventario_sites" COMMENT="Inventory sites/locations">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="nameunique" TYPE="unique" FIELDS="name"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_objects" COMMENT="Inventory objects/assets">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="description" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
                <FIELD NAME="typeid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0"/>
                <FIELD NAME="siteid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="available"/>
                <FIELD NAME="availableperiodenabled" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="availablefrom" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="availableto" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="availabletimes" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="currentlocation" TYPE="char" LENGTH="255" NOTNULL="false"/>
                <FIELD NAME="visible" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1" SEQUENCE="false"/>
                <FIELD NAME="createdby" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="sitefk" TYPE="foreign" FIELDS="siteid" REFTABLE="local_inventario_sites" REFFIELDS="id"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_properties" COMMENT="Custom property definitions">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="shortname" TYPE="char" LENGTH="100" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="parentid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0"/>
                <FIELD NAME="datatype" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="text"/>
                <FIELD NAME="options" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="required" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="sortorder" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="shortname_uq" TYPE="unique" FIELDS="shortname"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_types" COMMENT="Object types/templates">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="description" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="color" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="#2563eb"/>
                <FIELD NAME="requiresreturn" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1"/>
                <FIELD NAME="requireslocation" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="1"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_typeprops" COMMENT="Properties assigned to object types">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="typeid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="propertyid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="typefk" TYPE="foreign" FIELDS="typeid" REFTABLE="local_inventario_types" REFFIELDS="id"/>
                <KEY NAME="propfk" TYPE="foreign" FIELDS="propertyid" REFTABLE="local_inventario_properties" REFFIELDS="id"/>
                <KEY NAME="typepropuniq" TYPE="unique" FIELDS="typeid,propertyid"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_propvals" COMMENT="Values for custom properties">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="objectid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="propertyid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="value" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="objectfk" TYPE="foreign" FIELDS="objectid" REFTABLE="local_inventario_objects" REFFIELDS="id"/>
                <KEY NAME="propertyfk" TYPE="foreign" FIELDS="propertyid" REFTABLE="local_inventario_properties" REFFIELDS="id"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_reserv" COMMENT="Reservations of objects">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="objectid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="siteid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timestart" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timeend" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="location" TYPE="char" LENGTH="255" NOTNULL="false"/>
                <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="active"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="expirednotified" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="objectfk" TYPE="foreign" FIELDS="objectid" REFTABLE="local_inventario_objects" REFFIELDS="id"/>
                <KEY NAME="userfk" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id"/>
                <KEY NAME="sitefk" TYPE="foreign" FIELDS="siteid" REFTABLE="local_inventario_sites" REFFIELDS="id"/>
            </KEYS>
            <INDEXES>
                <INDEX NAME="timestart_idx" UNIQUE="false" FIELDS="objectid,timestart,timeend"/>
            </INDEXES>
        </TABLE>

        <TABLE NAME="local_inventario_license" COMMENT="License and API key status">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="apikey" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="domain" TYPE="char" LENGTH="255" NOTNULL="false"/>
                <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="free"/>
                <FIELD NAME="expiresat" TYPE="int" LENGTH="10" NOTNULL="false"/>
                <FIELD NAME="installid" TYPE="char" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="issuedat" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="protoken" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="protokenexpires" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="limitsjson" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="signature" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="lastcheck" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="lasttamper" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="lastpayload" TYPE="text" NOTNULL="false"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>

        <TABLE NAME="local_inventario_install" COMMENT="Installation/upgrade registration log">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="installid" TYPE="char" LENGTH="100" NOTNULL="false"/>
                <FIELD NAME="domain" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="version" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT=""/>
                <FIELD NAME="installedat" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="lastreported" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0"/>
                <FIELD NAME="status" TYPE="char" LENGTH="20" NOTNULL="true" DEFAULT="created"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
